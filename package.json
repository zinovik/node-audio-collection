  "engines": {
    "node": "12.0.0",
    "npm": "6.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zinovik/node-audio-collection.git"
  },
  "author": "Max Zinovik",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/zinovik/node-audio-collection/issues"
  },
  "homepage": "https://github.com/zinovik/node-audio-collection#readme",
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.7",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "moment": "^2.24.0",
    "mp3-duration": "^1.1.0",
