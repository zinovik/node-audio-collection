{
  "name": "node-audio-collection",
  "version": "0.0.14",
  "description": "node audio collection",
  "main": "lib/index.js",
  "bin": {
    "node-audio-collection": "./lib/index.js"
  },
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "test": "jest --config jestconfig.json --collectCoverage",
    "format": "prettier --write \"src/**/*.ts*\" \"test*/**/*.ts*\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint"
  },
  "engines": {
    "node": ">= 12",
    "npm": ">= 6.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zinovik/node-audio-collection.git"
  },
  "author": "Max Zinovik",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/zinovik/node-audio-collection/issues"
  },
  "homepage": "https://github.com/zinovik/node-audio-collection#readme",
  "devDependencies": {
    "@types/jest": "^24.0.21",
    "@types/node": "^12.12.5",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typemoq": "^2.1.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "moment": "^2.24.0",
    "mp3-duration": "^1.1.0",
    "music-metadata": "^4.8.4",
    "node-id3": "^0.1.11",
    "ts-node": "^8.4.1"
  },
  "files": [
    "lib/**/*"
  ]
}
